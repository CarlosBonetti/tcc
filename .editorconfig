# EditorConfig is awesome: http://EditorConfig.org

root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Matches multiple files with brace expansion notation
# Set default charset
charset = ISO-8859-1

[presentation.tex]
charset = utf-8

[utf8/*]
charset = utf-8

[artigo/*]
charset = utf-8
